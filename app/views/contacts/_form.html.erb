<%= simple_form_for(@contact) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.input :lastmeet, as: :date, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.input :frequency, class: 'form-control' %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
