<%= form_for(@contact) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.date :lastmeet %>
    <%= f.date :lastmeet, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.integer :frequency %>
    <%= f.integer :frequency, class: 'form-control' %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
